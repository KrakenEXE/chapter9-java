package projectFive;

public class CourseGrades {

	double[] grades = new double[4]; 
	double labs, exam, essay, finalExam;
	//general constructor for CourseGrades
	
	public CourseGrades(){
		
	}
	
	
	//methods described in details
	


	
	
	
	
	public String toString(){
		
		String str = "Your score for your labs was "+grades[0]+
				"\nYour score for your Pass Fail exam was "+grades[1]+"\nYour score for your Essay was "+grades[2]+
				"\nYour score for your Final Exam was "+grades[3];
		
		return str;
	}
}
