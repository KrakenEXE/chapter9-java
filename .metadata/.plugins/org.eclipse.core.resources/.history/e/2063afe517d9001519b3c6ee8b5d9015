package projectFive;

public class CourseDriver {

	public static void main(String[] args) {
		
		CourseGrades GradedActivity = new CourseGrades();
		CourseGrades PassFailExam = new CourseGrades();
		CourseGrades Essay = new CourseGrades();
		CourseGrades FinalExam = new CourseGrades();
		
		GradedActivity grades;
		PassFailExam passFailExam;
		Essay essay;
		FinalExam finalExam;
		
		grades.setLab(57);
		passFailExam.setPassFailExam(100);
		Essay.setEssay(100);
		FinalExam.setFinalExam(97);
		
		System.out.print(GradedActivity.toString());
	}

}
